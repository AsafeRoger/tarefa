programa
{
	inclua biblioteca Util
 --> u
	
	funcao inicio()
	{
		inteiro nume = 0
		cadeia chega = "n"
		enquanto (chega != "chega"){
			escreva("\n", u.sorteia(0, 10000000))
			nume = nume + 1 
			
			escreva("chega ja?")
			leia(chega)
		
			se(chega == "chega")
				escreva("numeros colocados" , nume)
			}
		}
}
